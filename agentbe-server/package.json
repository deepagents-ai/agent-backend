{
  "name": "agentbe-server",
  "version": "0.1.0",
  "type": "module",
  "description": "MCP server implementations for Agent Backend",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./servers/*": {
      "types": "./dist/mcp/servers/*.d.ts",
      "import": "./dist/mcp/servers/*.js",
      "require": "./dist/mcp/servers/*.cjs"
    }
  },
  "bin": {
    "agentbe-server": "./bin/agentbe-server.js"
  },
  "files": [
    "dist",
    "bin",
    "deploy",
    "src"
  ],
  "scripts": {
    "build": "vite build && tsc --project tsconfig.build.json",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:run": "vitest run"
  },
  "dependencies": {
    "constellationfs": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "express": "^4.21.2",
    "minimatch": "^10.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
