{
  "name": "agent-backend",
  "version": "1.0.0",
  "private": true,
  "description": "Agent Backend - client library and server infrastructure",
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "test:unit": "pnpm -r test:unit",
    "test:integration": "cd remote && pnpm run test:integration",
    "test:all": "pnpm test:unit && pnpm test:integration",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "dev": "pnpm -r --parallel dev"
  },
  "workspaces": [
    "agentbe-typescript",
    "remote"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "diff": "^8.0.2"
  }
}