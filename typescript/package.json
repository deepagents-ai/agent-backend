{
  "name": "agent-backend",
  "version": "0.8.9",
  "description": "A distributed filesystem backend for your AI agent in a single API, just like a database or storage system.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agent-backend": "./bin/agent-backend.js"
  },
  "files": [
    "dist",
    "bin",
    "deploy",
    "native",
    "scripts",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:run": "vitest run",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "filesystem",
    "ai",
    "agents",
    "bash",
    "typescript",
    "tools"
  ],
  "author": "AgentBackend Contributors",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agent-backend/agent-backend.git"
  },
  "bugs": {
    "url": "https://github.com/agent-backend/agent-backend/issues"
  },
  "homepage": "https://github.com/agent-backend/agent-backend#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.10.0",
    "@types/minimatch": "^5.1.2",
    "buffer": "^6.0.3",
    "express": "^5.1.0",
    "hono": "^4.11.7",
    "minimatch": "^10.1.1",
    "ssh2": "^1.16.0",
    "ws": "^8.19.0",
    "zod": "^4.1.5"
  },
  "optionalDependencies": {
    "node-gyp": "^10.0.0"
  },
  "peerDependencies": {
    "@ai-sdk/mcp": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/mcp": {
      "optional": true
    }
  },
  "devDependencies": {
    "@ai-sdk/mcp": "^1.0.18",
    "@eslint/js": "^9.39.2",
    "@types/express": "^5.0.2",
    "@types/node": "^22.10.0",
    "@types/ssh2": "^1.15.5",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "prettier": "^3.4.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.1",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.js"
    },
    "./mcp": {
      "types": "./dist/mcp/index.d.ts",
      "import": "./dist/mcp/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js"
    },
    "./package.json": "./package.json"
  }
}
